<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
	<table name="TB_USER" schema="dbo" catalog="BIDB"
		class="sys.common.model.TBUser">
		<primary-key>
			<generator class="assigned"></generator>
		</primary-key>
	</table>
	<table name="TB_ACTION" schema="dbo" catalog="BIDB"
		class="TBAction">
		<primary-key>
			<generator class="assigned"></generator>
		</primary-key>
	</table>
	<table name="TB_MENU" schema="dbo" catalog="BIDB"
		class="sys.common.model.TBMenu">
		<primary-key>
			<generator class="assigned"></generator>
		</primary-key>
	</table>
<table name="TB_ROLE" schema="dbo" catalog="BIDB"
	class="sys.common.model.TBRole">
	<primary-key>
		<generator class="assigned"></generator>
	</primary-key>
</table>
</hibernate-reverse-engineering>